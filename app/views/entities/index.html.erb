<div class="container border-b border-gray-900/10 pb-12">
  <h1 class="text-base font-semibold text-4xl text-blue-500">GXWeb Entities</h1>
  <table class="table tftable" border="1">
    <thead>
      <tr>
        <th>ID</th>
		    <th>Title</th>
		    <th>Description</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% @entity.each do |entity| %>
        <tr>
          <td><%= entity.id %></td>
          <td><%= entity.title %></td>
          <td><%= entity.description %></td>
          <td><%= link_to "Login", login_entity_path(entity) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
